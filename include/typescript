// c--------------------------------------------------------------------------

main( int argc, char **argv )
{
    char filename[256];
    char *uncompressed_dir = NULL;
    char *compressed_dir = NULL;
    char *grid_dir = NULL;
    char *nexrad_dir = NULL;
    char *tape_dev = NULL;
    char *radar_name = NULL;
