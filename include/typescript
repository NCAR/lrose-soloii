>       }
> 
>       if (!(produce_4_dsd_fields || produce_5_dsd_fields)) {
2864a2899,2988
> int do_dsd_stuff2( int num_gates )
> {
>   typedef short* SHORTPTR;
> 
>   SHORTPTR n0_ptr, mu_ptr, lam_ptr, d0_ptr, rr_ptr, dkd_ptr, res_ptr, nt_ptr;
>   SHORTPTR rnx_ptr, rzd_ptr, rkd_ptr, lwc_ptr;
>   n0_ptr = (short *)maprO->raw_data_ptr( fn_n0 );
>   mu_ptr = (short *)maprO->raw_data_ptr( fn_mu );
>   lam_ptr = (short *)maprO->raw_data_ptr( fn_lam );
>   d0_ptr = (short *)maprO->raw_data_ptr( fn_d0 );
>   rr_ptr = (short *)maprO->raw_data_ptr( fn_rr );
>   dkd_ptr = (short *)maprO->raw_data_ptr( fn_dkd );
>   res_ptr = (short *)maprO->raw_data_ptr( fn_res );
>   nt_ptr = (short *)maprO->raw_data_ptr( fn_nt );
>   rnx_ptr = (short *)maprO->raw_data_ptr( fn_rnx );
>   rzd_ptr = (short *)maprO->raw_data_ptr( fn_rzd );
>   rkd_ptr = (short *)maprO->raw_data_ptr( fn_rkd );
>   lwc_ptr = (short *)maprO->raw_data_ptr( fn_lwc );
> 
>   int gg = 0, ii, mark;
> 
>   double N0, mu, lam, D0, res, Nt, rain, kdp, rnx, rzd, rkd, lwc;
>   double nt_scale = 1000., scale = 100., lwc_scale;
>   int tf = 0;
>   nt_scale = maprO->parms[ fn_nt ]->parameter_scale;
>   lwc_scale = maprO->parms[ fn_lwc ]->parameter_scale;
>   short bad =  (short)maprO->parms[fn_n0]->bad_data;
> 
>   double *gatesZdr = pbiZdr->return_gate_array(Averaged_Data);
>   ii = (medFltrVal >= 0) ? Median_Filtered_Data : Averaged_Data;
>   double *gatesZh = pbiZh->return_gate_array(ii);
>   double *gatesKdp = pbiKdp->return_gate_array( Unscaled_Base_Data );
>   short *pd = (short *)maprO->raw_data_ptr( fn_pd );
>   short pd_thr = (short)(5 * pd_scale);
> 
> 
> 
>   for( gg = 0; gg < num_gates; gg++, tf = 0 ) {
> 
>     if (pd[gg] > 0 && pd[gg] <= pd_thr) {
>       rdsd2( dBz_to_Z( gatesZh[gg] )
> 	     , dBz_to_Z(gatesZdr[gg])
> 	     , gatesKdp[gg], mdmu, mdph
> 	     , N0, mu, lam, D0, res, Nt, lwc, rain, rnx, rzd, rkd, kdp, tf);
>     }
> 
>     if( tf ) {
>       mu_ptr[gg] =  (short)DD_SCALE( mu, scale, 0 );
>       d0_ptr[gg] =  (short)DD_SCALE( D0, scale, 0 );
>       rr_ptr[gg] =  (short)DD_SCALE( log10(rain), scale, 0 );
>       res_ptr[gg] = (short)DD_SCALE( res, scale, 0 );
>       if (!produce_4_dsd_fields) {
> 	lwc_ptr[gg] = (short)DD_SCALE( lwc, lwc_scale, 0 );
>       }
> 
>       if (!(produce_4_dsd_fields || produce_5_dsd_fields)) {
> 	n0_ptr[gg] =  (short)DD_SCALE( log10(N0), scale, 0 );
> 	lam_ptr[gg] = (short)DD_SCALE( lam, scale, 0 );
> 	dkd_ptr[gg] = (short)DD_SCALE( kdp, scale, 0 );
> 	nt_ptr[gg] =  (short)DD_SCALE( log10(Nt), nt_scale, 0 );
> 	rnx_ptr[gg] = (short)DD_SCALE( log10(rnx), scale, 0 );
> 	rzd_ptr[gg] = (short)DD_SCALE( log10(rzd), scale, 0 );
> 	rkd_ptr[gg] = (short)DD_SCALE( log10(rkd), scale, 0 );
>       }
>     }
>     else {
>       mu_ptr[gg] =  bad; 
>       d0_ptr[gg] =  bad;
>       rr_ptr[gg] =  bad;
>       res_ptr[gg] = bad;
>       if (!produce_4_dsd_fields) {
> 	lwc_ptr[gg] = bad;
>       }
> 
>       if (!(produce_4_dsd_fields || produce_5_dsd_fields)) {
> 	n0_ptr[gg] =  bad;
> 	lam_ptr[gg] = bad;
> 	dkd_ptr[gg] = bad;
> 	nt_ptr[gg] =  bad;
> 	rnx_ptr[gg] = bad;
> 	rzd_ptr[gg] = bad;
> 	rkd_ptr[gg] = bad;
>       }
>     }
>   }
>   return 0;
> }
> 
> // c---------------------------------------------------------------------------
> 
2998a3123,3478
> // c--------------------------------------------------------------------------
> void test_dsd()
> {
>   /*
> c---------------------------------------------------------------
> c       up dated on 6/7/01 using truncated lambda-mu relation
> c       lambda=0.0365mu^2+0.735mu+1.935
> c       and add classical rain rate estimation
> C       NexRad R(Z), ZD: R(Z, Zdr), and R(kdp)
> c---------------------------------------------------------------
> c       up dated on 10/11/01 using
> c       experimental fitted axis ratio
> c       aovrb=0.9951+0.0251*d-0.03664*d*d+5.303e-3*d*d*d-2.492e-4*d*d*d*d
> c       and ED derived new polinomial-fitting velocity
> c       v = -0.1021 + 4.932D - 0.9551D^2 + 0.07934 D^3 - 0.002362D^4
> c---------------------------------------------------------------
> c       revised on 10/22/01
> c       1) increase lambda from 0.5 to 10.5
> c       2) include D_0 calculation for Zdr < 0.3dB
> c---------------------------------------------------------------
> c	revised on 11/10/01
> c	1) use lambda - ZDR table
> c	2) use bisection method for speed the lambda finding
> c	3) includes fixed mu DSDs like exponential distribution 
> c---------------------------------------------------------------
> c	S-Pol measurements Z, ZDR, and a constrained 
> c	relation: mu-lambda relation
>    */
>   double Z, Zdr,rain,kdp,N0,mu,lam,D0,phidp;
>   double lwc, res, Nt,rkdp,RNX,RZD,RKD;
>   double raine,N0e,mue,lame,D0e, lwce, rese, Nte;
>   double zdr, zdrdb, dbz; 
>   int tf, mdmu, mdph;
>   //c	input Z, Zdr in linear scale
>   phidp=10.0;
>   dbz=40;
>   zdrdb=2.5527;
>   kdp=1.0;
>   dbz=dbz+0.0198*phidp;
>   zdrdb=zdrdb+0.0038*phidp;
>   //c	dbz=35.66;
>   //c	zdrdb=1.0;
>   // Z=10**(dbz/10.0);
>   Z=pow(10.,(dbz/10.0));
>   // Zdr=10**(zdrdb/10.0);
>   Zdr=pow(10,(zdrdb/10.0));
>   //c	Z=10000;
>   //c	Zdr=1.8;
>   mdmu=1;	//!1:mu-lambda,2:mu=0,3:mu=2,4:mu=4
>   mdph=1;	//!1:sig_phi=0,2:sig_phi=10,3:sig_phi=15
> 
>   rdsd2(Z, Zdr,kdp, mdmu, mdph,N0, mu, lam,D0,res,Nt,
> 	      lwc, rain,RNX,RZD,RKD,rkdp,tf);
> 
>   printf ("Z:%.4f,Zdr:%.4f,kdp:%.4f,N0:%.4f,mu:%.4f,lam:%.4f
> D0:%.4f,res:%.4f,Nt:%.4f,lwc:%.4f,rain:%.4f
> RNX:%.4f,RZD:%.4f,RKD:%.4f,rkdp:%.4f
> tf:%d\n"
> 	  , Z,Zdr,kdp,N0,mu,lam,D0,res,Nt,lwc,
> 	  rain,RNX,RZD,RKD,rkdp,tf);
> 
> }
> 
> // c---------------------------------------------------------------------------
> 
> int rdsd2( double Z, double Zdr, double nkdp, int mdmu, int mdph
> 	   , double &N0, double &mu
> 	   , double &lam, double &D0, double &res, double &Nt
> 	   , double &lwc, double &rain, double &rnx
> 	   , double &rzd, double &rkd
> 	   , double &kdp, int &tf
> 	   )
> {
>   // c...guzhang  c...dsd  c...mark
> 
>   static const int maxSigmas = 24;
>   static int numLambdas = 0, nt, ip;
>   static double ala, alb, alf, bta, btb, btf, br, bk;
>   static double wave, phi, phi2, K2, PI_e4, wave_e4, phi2_coeff;
>   static double cc1 = 8.97, cc2 = 0.866;
>   static double *d, *fh, *fv, *fd, *ndt;
>   static double c0=-0.1021,c1=4.932,c2=-0.9551,c3=0.07934,c4=-0.002362; 
> 
>   double mu1, mu2, fm, fp;
>   double fhh2, dlm, ga, gab, D1, D2, gr, C1, gK2, ZDP;
>   double zh0, tkd, Dm0, rain0, dd, nd, v, ndd, zdrdb;
> 
>   int i, j, k, mm=128, nn=80, mx=70, id, md;
>   int gg, hh;
>   static complx ix, jx;
>   complx epr, a, b, c;
>   char line[256], *sptrs[32];
>   double_complex iix( 1, 0 ), jjx( 0, 1 ), gga, ggb, ggc, gepr;
>   int ln;
>   
> 
>   if( !zdrB_lut ) {		// first time through
> 
>     char *fname, *buf = 0;
>     size_t len, lenx;
>     static char **lptrs = new char * [512];
> 
>     if (fname = getenv ("DSD_ZDR_TABLE")) {
>       buf = absorb_ascii_file (fname);
>     }
>     if (!buf) {
>       nt = sizeof (dsd_zdr_table_ptrs)/sizeof (char *);
>       memcpy (lptrs, dsd_zdr_table_ptrs, sizeof (dsd_zdr_table_ptrs));
>     }
>     else {
>        nt = dd_tokenz (buf, lptrs, "\n");
>     }
> 
>     zdrB_lut = new double* [maxSigmas];
>     lambda_lut = new double [nt];
> 
>     // absorb table lambda/zdr table
>     // ignore the first two lines
> 
>     i = 0;
>     for (ln=2; ln < nt; ln++) {
>       strcpy (line, lptrs[ln]);
>       nn = dd_tokens (line, sptrs);
>       if (!i) {
> 	// allocate so as to pass a column into the search routine
> 	for( gg = 0; gg < nn-1; gg++ ) { 
> 	  zdrB_lut[gg] = new double [nt];
> 	}
>       }
>       j = 0;
>       lambda_lut[i] = atof( sptrs[0] );
> 
>       for( mm = 1; mm < nn; j++, mm++ )	// zdr values
> 	{
> 	  zdrB_lut[j][i] = atof( sptrs[mm] );
> 	}
>       numLambdas = ++i;
>     }
>     if (buf)
>       delete [] buf;
> 
>     // absorb the x table
>     // absorb the raindrop diameter vs scattering table
>     //
>     //
>     nt = sizeof (dsd_x_table_ptrs)/sizeof (char *);
>     memcpy (lptrs, dsd_x_table_ptrs, sizeof (dsd_zdr_table_ptrs));
>     d = new double [nt];
>     fh = new double [nt];
>     fv = new double [nt];
>     fd = new double [nt];
>     ndt = new double [nt+1];
> 
>     for (ln=0; ln < nt; ln++) {
>       strcpy (line, lptrs[ln]);
>       nn = dd_tokens (line, sptrs);
> 
>       d[ln]  = atof (sptrs[0]);
>       fh[ln] = atof (sptrs[1]);
>       fv[ln] = atof (sptrs[2]);
>       fd[ln] = atof (sptrs[3]);
>     } 
> 
>     ala = 4.26e-4; alb = 4.76e-4; alf = 1.33e-5;
>     bta = 3.02; btb = 2.69; btf = 4.61;
>     ix.re = 1; ix.im = 0;
>     jx.re = 0; jx.im = 1;
> 
>     // phi2=(sigma*pi/180.0)**2
>     phi = RADIANS( sigma );
>     phi2 = pow( phi, 2 );
>     phi2_coeff = ( 1 -2 *phi2 );
>     br = -3.1303 +0.6672 *phi -9.773 *phi2;
>     bk = -2.114 +0.4232 *phi -6.2713 *phi2;
>     ip = (int)(sigma/2);
>     
>     
>     // epr=80.205+jx*17.167
>     
>     epr = complx_mult_const( jx, 17.167 );
>     epr = complx_add_const( epr, 80.205 );
>     
>     //K2=cabs((epr-1)/(epr+2))**2
>     
>     a = complx_add_const( epr, -1 );
>     b = complx_add_const( epr, 2 );
>     c = complx_div( a, b );
>     K2 = pow( complx_abs(c), 2);
>     
>     wave = 107.0;
>     wave_e4 = pow( wave, 4 );
>     PI_e4 = pow( PI, 4 );
>     nt = 96;
> 
>   } // end of one time only stuff
> 
> 
>   // tradtional calculations of ZR ranfall rate
>   // Zdr rainfall rate
>   // Kdp rainfall rate
> 
>   rnx = pow (Z*3.3333e-3, 0.7142857);
>   rzd = (Zdr) ? 6.84e-3 * Z/pow (Zdr, 4.86) : 0;
>   rkd = 40.56 * pow (nkdp, 0.866);
> 
> 
>   // mdmu determines the dsd shape to be used
>   // and has the following significance 1:mu-lambda, 2:mu=0, 3:mu=2, 4:mu=4 
> 
>   // mdph determines which std. dev. of the canting angle is to be used
>   // and has the following significance 1:sig_phi=0, 2:sig_phi=10, 3:sig_phi=15
> 
>   md = mdmu +(mdph-1)*4;	// at this point this is a Fortran subscript
> 
>   // "md" determines which set of conditions (i.e. which lookup table)
>   // will be used in deriving lambda from Zdr
> 
>   // N0 is the offset
>   // mu is the shape
>   // lam[da] is the slope
>   // D0 is the medium volume dia.
>   // rain is the DSD rainfall rate
>   // kdp as derived from DSD info
>   // res is radar estimated size
>   // Nt is total number concentration
>   // lwc is liquid water content
> 
>   //  dlm = tbl(2,13)-tbl(1,13)	
>   dlm = lambda_lut[1] -lambda_lut[0];
> 
>   // fm=zdr-tbl(mm,md)
>   fm = Zdr -zdrB_lut[md-1][numLambdas-1];
> 
>   // fp=zdr-tbl(1,md)
>   fp = Zdr -zdrB_lut[md-1][0];
> 
>   if (fm*fp > 0.0) {
>     tf=0;
>     N0=-99;
>     mu=-99;
>     lam=-99;
>     D0=-99;
>     res=-99;
>     Nt=-99;
> 
>     // lwc=2.432e-4*Z*zdr**(4.26)
>     lwc = 2.432e-4 * Z * pow(Zdr,4.26);
> 
>     // rain=7.46e-3*Z**0.945*zdr**(-4.76)
>     rain = 7.46e-3 * pow(Z,0.945) * pow(Zdr,4.26);
> 
>     // rkdp=3.24e-3*Z*zdr**(-1.98)
>     kdp = 3.24e-3 * Z * pow(Zdr,-1.98);
> 
>     // zdrdb=10*log10(zdr)
>     zdrdb = 10. * log10(Zdr);
> 
>     if(zdrdb <= 0.35 && mdmu == 1) { 
>       // D0=0.679+1.606*zdrdb-0.8682*zdrdb**2
>       D0 = 0.679 + 1.606 * zdrdb - 0.8682 * zdrdb * zdrdb;
>     }
>     return 1;
>   }
> 
>   id = dsd_bin_srch( Zdr, zdrB_lut[md-1], numLambdas); // lambda index
> 
>   // fm=zdr-tbl(id,md)
>   fm = Zdr -zdrB_lut[md-1][id];
> 
>   if (fm > 0) {
>     id--;
>     fm = Zdr -zdrB_lut[md-1][id];
>   }
>   // fp=zdr-tbl(id+1,md)
>   fp = Zdr - zdrB_lut[md-1][id+1];
> 
>   //  lam = lambda_lut[id] + fabs(fp/(fm-fp)) * dlm;
>   lam = lambda_lut[id] + fabs(fm/(fm-fp)) * dlm;
> 
>   tf = 1 ;
>   if(mdmu == 1) {
>     // mu=(-0.735+(0.735**2+4*0.0365*(lam-1.935))**0.5)/2/0.0365
>     mu = (-0.735 + pow(0.735*0.735 + 4*0.0365*(lam-1.935),.5)) /2 /0.0365;
>   }
>   else
>     { mu = (mdmu-2) * 2.0; }
> 
>   if(mdph == 1)
>     { phi = 0.0; }
>   else
>     { phi = 10.0 + (mdph-2) * 5.0; }
> 
>   zh0 = 0.0;
>   tkd = 0.0;
>   Dm0 = 0.0;
>   ndt[0] = 0.0;
>   rain0 = 0.0;
>   dd = d[1] -d[0];
> 
>   // "d" is the diameter of the raindrops in mm
>   // "fh" is the horizontal scattering amplitude
>   // "fv" is the vertical
>   // "fd" is the differential
> 
> 
>   for (j=0; j < mx; j++) {
>     // weighted total area under dsd curve
> 
>     //  nd=d(j)**mu*exp(-lam*d(j))
>     nd = pow (d[j],mu) * exp(-lam*d[j]);
> 
>     //  zh0=zh0+((1-2*phi2)*fh(j)**2+2*phi2*fh(j)*fv(j))*nd*dd
>     zh0 = zh0 + ((1-2*phi2)*pow(fh[j],2) + 2*phi2*fh[j]*fv[j])*nd*dd;
> 
>     //  tkd=tkd+(1-2*phi2)*fd(j)*nd*dd
>     tkd=tkd+(1-2*phi2)*fd[j]*nd*dd;
> 
>     //  ndt(j+1)=ndt(j)+nd*d(j)**3*dd
>     ndt[j+1] = ndt[j] + nd*pow(d[j],3)*dd;
> 
>     //  v=c0+c1*d(j)+c2*d(j)**2+c3*d(j)**3+c4*d(j)**4
>     v = c0 + c1*d[j] + c2*pow(d[j],2) + c3*pow(d[j],3) + c4*pow(d[j],4);
>   
>     //  rain0=rain0+pi/6.0*3.6e-3*v*d(j)**3*nd*dd
>     rain0 = rain0 + PI/6.0*3.6e-3*v*pow(d[j],3)*nd*dd;
> 
>     //  Dm0=Dm0+d(j)**0*nd*dd ????????
>     Dm0 = Dm0 + nd*dd;
>   }
> 
>   fhh2 = Z*PI_e4*K2/4.0/wave_e4;
>   N0 = fhh2/zh0;
>   Nt = Dm0*N0;
>   //  lwc=1e-3*pi/6*N0*ndt(mx+1)
>   lwc = 1e-3 * PI/6*N0*ndt[mx];
> 
>   rain = rain0*N0;
>   //  rkdp=wave*N0*tkd*180/pi/1e+3
>   kdp = DEGREES(wave*N0*tkd)/1e+3;
>   D0 = (mu+3.67)/lam;
>   ZDP = Z*(1.0-1.0/Zdr);
>   res = cc1*pow(ZDP/Z,cc2);
> 
>   for (ndd = 0,id = 1; id < mx; id++) {
>     // find probability of .5 corresponding to particle size
> 
>     ndd = ndt[id]/ndt[mx];
>     if (!(ndd < .5))
>       { break; }
>   }
>   D0 = d[id] - (ndd-0.5)/(ndd - ndt[id-1]/ndt[mx])*dd;
> 
