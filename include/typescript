        if( strstr( source_dev, "/dev/" ) ) {
            tape_dev = source_dev;
        }
    }
    else {
        source_dev = "/scr/hotlips/oye/ktlx.tape";
    }
    printf("Opening: %s \n", source_dev);
    if((tpid=open(source_dev,0)) < 0 ) {
      printf("Unable to open: %s state: %d\n", source_dev, tpid);
      exit(1);
    }
[63;1H[K:[63;1H[63;1H[K
    nexrad_dir = getenv( "NEXRAD_DIR" );
    if( !nexrad_dir ) { printf( "No NEXRAD_DIR " ); }

    grid_dir = getenv( "GRID_DIR" );
    if( !grid_dir ) { printf( "No GRID_DIR " ); }

    uncompressed_dir = getenv( "UNCOMPRESSED_DIR" );
    compressed_dir = getenv( "COMPRESSED_DIR" );
    if( !compressed_dir && !uncompressed_dir )
      { printf( "No (UN)COMPRESSED_DIR " ); }

    if( !nexrad_dir || !grid_dir || (!compressed_dir && !uncompressed_dir ))
      { printf( "\n" ); exit( 1 ); }

    // need an object that iterates through a list of media
    // whether it's a stacker full of tapes
    // one tape in a single drive
    // or one or more files


    int max_rec_size = 65536;
    char * raw_data = new char [max_rec_size];
    char *at;

    nex_mapper * nm = new nex_mapper( nexrad_dir );

    int jj;
    DTime tm, start_time = 0, stop_time = 0x7fffffff;
    int tm_set = 0;

    if( aa = getenv( "TIME_LIMITS" )) {
      if( !llg_nab_times( aa )) {
        printf( "**** NO useable time limits!\n" );
        exit( 1 );
      }      
      start_time = *( the_times + tm_set );
      stop_time = *( the_times + tm_set +1 );
      tm_set++;
    }  


    if( radar_name = getenv( "RADAR_NAME" )) {
        jj = nm->use_site_name( radar_name );
    }

    if( aa = getenv( "RADAR_LATITUDE" )) {
        nm->set_radar_latitude( atof( aa ) );
    }
    if( aa = getenv( "RADAR_LONGITUDE" )) {
        nm->set_radar_longitude( atof( aa ) );
    }
    if( aa = getenv( "RADAR_ALTITUDE" )) {
        nm->set_radar_altitude_mmsl( atof( aa ) );
    }
    if( aa = getenv( "PROJECT_NAME" )) {
    }
# ifdef notyet
    if( aa = getenv( "" )) {
    }
# endif

[63;1H[K:[63;1H[63;1H[K    double total_bytes = 0;
    int count = 0, count_trip = 176, mark;
    int file_count = 0;
    int eof_count = 0, bytes_used, len, rlen, rcount, ttrcount;
    int tstatus, ts_err_count = 0, exit_err = 0;
    int ngrids = 0, nres = 0;
    double dvals[8];
    char *sptrs[32], str2[256];


    lvl2_grids * lvl2 = new lvl2_grids( grid_dir );


    if( aa = getenv( "GRID_RADII" )) {
      strcpy( str2, aa );
      ngrids = dd_tokens( str2, sptrs );
      for( ii = 0; ii < ngrids; ii++ )
        { *( dvals +ii ) = atof( sptrs[ii] ); }
      lvl2->set_grid_radii( ngrids, dvals ); // km.
    }
    if( aa = getenv( "RESOLUTIONS" )) {
      strcpy( str2, aa );
      nres = dd_tokens( str2, sptrs );
      for( ii = 0; ii < nres; ii++ )
        { *( dvals +ii ) = atof( sptrs[ii] ); }
      lvl2->set_grid_resolutions( nres, dvals ); // degrees
    }

    if( ngrids || nres ) {
      if( ngrids && nres && ngrids != nres ) {
        printf( "Number of grids: %d does not agree with ", ngrids );
        printf( "the number of resolutions: %d", nres );
        exit(1);
      }
    }

    if( getenv( "XY_GRID" ))
      { lvl2->setXYgrid(); }
    else if( getenv( "OLD_LATLON_GRID" ))
      { lvl2->set_old_latlon_grid(); }

    if( getenv( "NO_AVERAGING" )) {
      lvl2->set_no_averaging();
    }

    if( uncompressed_dir )
      { lvl2->setCompressedDir( uncompressed_dir, YES ); }
    else
      { lvl2->setCompressedDir( compressed_dir, NO ); }
    int ok, break_break = NO;



    // for each record


    for( ;; ) {
        count++;
        if( count >= count_trip ) {
            mark = 0;
        }

[63;1H[K:[63;1H[63;1H[K        if( !tape_dev ) {
            rlen = fb_read( tpid, raw_data, max_rec_size );
            if(rlen == 0) {
                printf( "ol:EOF %2d %7.3fMB  %d\n"
                        , ++file_count, total_bytes*1.e-6, count );
                if(++eof_count >= 3) {
                    printf("Exit after 3 consecutive EOFs--count: %d\n"
                           , count);
                    break;
                }
                continue;
            }
            if(rlen < 0) {
                perror( " " );
                printf( "ol:Last read status: %d\n", rlen );
                break;
            }
        }
        else {                  // Tape!
            rlen = read( tpid, raw_data, max_rec_size );

            if(rlen == 0) {     // TS_EOF
                printf( "ol:EOF %2d %7.3fMB  %d\n"
                        , ++file_count, total_bytes*1.e-6, count );

                if(++eof_count >= 3) {
                    printf("Exit after 3 consecutive EOFs--count: %d\n"
                           , count);
                    exit_err = 0;
                    break;
                }
                continue;
            }
            if(rlen < 0) {      // TS_ERROR
                printf( "EOT or Tape read error--count: %d\n"
                        , count, ts_err_count );
                if( ++ts_err_count < 5 )
                    { continue; }
                exit_err = 1;
                break;
            }
        }
        total_bytes += rlen;
        ts_err_count = 0;
        eof_count = 0;
        rcount = 0;
        at = raw_data;

        // for each ray

        for( ;; ) {
            rcount++;
            nm->map_ptrs( at, rlen, &bytes_used );

            if( nm->new_ray() ) {
              if( num_times > 0 ) {
                tm = nm->d_time();

                if( tm >= stop_time && tm_set >= num_times ) {
                  break_break = YES;
                  break;
                }
[63;1H[K:[63;1H[63;1H[K                if( tm > stop_time ) {
                  jj = tm_set++ * 2;
                  start_time = *( the_times + jj );
                  stop_time = *( the_times + jj +1 );
                }
                if( start_time <= tm && tm < stop_time ) {
                  lvl2->next_ray( nm );
                }
              }
              else {
                lvl2->next_ray( nm );
              }
              mark = 0;
            }

            rlen -= bytes_used;

            if( rlen < NEX_PACKET_SIZE ) {
                break;
            }
            at += bytes_used;
        }
        ttrcount += rcount;
        if( break_break )
          { break; }
    }


    int final_volume = YES;
    lvl2->finish( nm, final_volume );
    mark = 0;
    exit( exit_err );
}

