[?1h=[63;1H[K#!/bin/csh
# script to control gridding of radar data
#
setenv SAVE_TIME_SPAN 14400 # 4 hours
#
setenv DORADE_DIR /scr/hotlips2/oye/data/marshall/xltrs
setenv DORADE_DIR /scr/hotlips2/oye/sweepfiles/save/
setenv DORADE_DIR /scr/rerun/oye/data/test/forGridr
setenv DORADE_DIR /scr/data/spol/sweepfiles/forGrids
#
setenv NETCDF_DIR /scr/hotlips2/oye/data/nc/grids/
setenv NETCDF_DIR /scr/rerun/oye/data/test/nc/grids
setenv NETCDF_DIR /scr/util-dt/spol/data/nc/grids
setenv NETCDF_DIR /scr/data/spol/nc/grids
#
setenv NETCDF._LINKS /scr/util-dt/spol/data/nc/grids/forAdj
setenv NETCDF._LINKS /scr/data/spol/nc/grids/forAdj
#
#setenv MDV_DIR    /scr/hotlips2/oye/data/ndv/grids/
#
#setenv ONLY_ONE_PASS
#
setenv OPTIONS ""
#
setenv OUTPUT_FIELDS " DZ VE ZD "
#
# thresholding  remove_lt  remove_gt  remove_lt_and_gt     
#
setenv DZ " source:dz;  threshold:dm,remove_lt,-116; "
setenv VE " unfold;  source:vr;  threshold:ncp,remove_lt,0.25; "
setenv ZD " source:zd;  threshold:dm,remove_lt,-116; "
#
setenv GRIDDING_ALGORITHM "XY_EL; CRESSMAN; AZRADIUS:1.5;" # degrees
#
#setenv GRID_NAMES " SUR NORTH SOUTH EAST WEST "
#
#setenv GRID_NAMES " SUR SC_EAST SC_WEST SC_TOW SC_AWA \
#       SK_NE SK_SW SK_TOW SK_AWA KC_SE "
setenv GRID_NAMES " SUR PPI"

# grids are assumed to be relative to the radar
#
#              " xmin  xmax xinc   ymin  ymax yinc         sectors
setenv SUR     " -125.  125.   1.  -125.  125.   1.   "  # (  0,360)
setenv PPI     " -125.  125.   1.  -125.  125.   1.   "  # (  0,360)
#
setenv SC_EAST "  -70.  125.   1.  -125.   70.   1.   "  # ( 75,195)
setenv SC_WEST " -125.    0.   1.  -125.  110.   1.   "  # (195,315)
setenv SC_TOW  " -125.  110.   1.  -125.    0.   1.   "  # (135,255)
setenv SC_AWA  " -110.  125.   1.     0.  125.   1.   "  # (315, 75)
# 
setenv SK_NE   "    0.  125.   1.  -125.  125.   1.   "  # ( 22,142)
setenv SK_SW   " -125.  100.   1.  -125.    0.   1.   "  # (142,262)
setenv SK_TOW  "  -70.  140.   1.  -125.  125.   1.   "  # ( 82,202)
setenv SK_AWA  " -125.   60.   1.   -25.  125.   1.   "  # (262, 22)
#
setenv KC_SE   "  -70.   95.   1.  -140.    0.   1.   "  # (142,195)
#
/ops/bin/swpfiGrids
#
[63;1H[K[7mgrids (END)[m[63;1H[63;1H[K[7m(END)[m[63;1H[K[?1l>zin: less ncgrids

[?1h=[63;1H[K#!/bin/csh
umask 002
# script to control gridding of radar data
#
setenv COMPRESSION_COMMAND " none "
#
setenv NC_SAVE_TIME_SPAN 14400 # 4 hours
#
setenv SWPFI_DIR /scr/bihub/links
setenv SWPFI_DIR /scr/bihub2/links
#
setenv NETCDF_DIR /scr/data/spol/nc/grids
#
#setenv NETCDF_LINKS /scr/data/spol/nc/grids/forT1
#
setenv OPTIONS " BISTATIC ONE_FILE_PER_SWEEP" # causes each gridded sweep to be outpu
#
setenv OUTPUT_FIELDS " U_BEST V_BEST"
setenv OUTPUT_FIELDS " U_BEST V_BEST DZ0 VE0"
#
setenv DZ0 "source: DZ0;"
#
setenv VE0 " source: VE0; long_name: Doppler velocities; units: m/s; "
#
setenv U_BEST " source:u_best; long_name: EW Vel. Component; units: m/s;"
setenv U_BEST " source:u_best; long_name: EW Vel. Component; units: m/s; \
       threshold:u_best,remove_lt_and_gt,-50,50;"
#
setenv V_BEST " source:v_best; long_name: NS Vel. Component; units: m/s;"
setenv V_BEST " source:v_best; long_name: NS Vel. Component; units: m/s; \
       threshold:v_best,remove_lt_and_gt,-50,50;"
#
setenv GRIDDING_ALGORITHM " CRESSMAN; AZRADIUS:1.5; " # degrees
#
setenv GRID_NAMES " DEFAULT "
#
# grids are assumed to be relative to the radar
#
#              "  xmin   xmax  xinc   ymin   ymax  yinc   "
setenv DEFAULT "  -60.     0.    1.   -60.    60.    1.   "
setenv DEFAULT " -59.5    59.5   1.   -59.5   59.5    1.   "
#
#setenv ONLY_ONE_PASS
#
/ops/bin/ncswpfiGrids
#gdb /home/oye/code/oye/spol/misc/ncswpfiGrids
exit 0
#
#
#
#
#
#
#
setenv VREF " reference_velocity;  sou.rce: vr; link_to:VE; \
       long_name: Unfold reference velocities; units: m/s;"

setenv NCP " source: ncp; units:none; "
setenv NETCDF._LINKS /scr/util-dt/spol/data/nc/grids/forAdj
setenv SWPFI_DIR /scr/data/spol/sweepfiles/forGrids
setenv OUTPUT_FIELDS " DZ VE VREF NCP "
setenv DEFAULT " -149.5  149.5   1.  -149.5  149.5   1.   "
