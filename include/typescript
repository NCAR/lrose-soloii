[63;1H[K:[63;1H[HM                        , &nvh->VCP_num
[63;1H[K:[63;1H[HM                        , &nvh->pattern_type
[63;1H[K:[63;1H[HM                        , &nvh->item_1
[63;1H[K:[63;1H[HM                        , str
