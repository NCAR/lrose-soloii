[?1h=[63;1H[K/*      $Id$      */

# include "hnxg_mgr.hh"

extern "C"
{
  double d_angdiff( double a1, double a2 );
  double dd_earthr(double lat);

  int loop_ll2xy_v3( double *plat, double *plon, double *palt
                     , double *x, double *y, double *z
                     , double olat, double olon, double oalt
                     , double R_earth, int num_pts );

  int loop_xy2ll_v3( double *plat, double *plon, double *palt
                     , double *x, double *y, double *z
                     , double olat, double olon, double oalt
                     , double R_earth, int num_pts );
    
};

// c--------------------------------------------------------------------------
// c--------------------------------------------------------------------------
// c--------------------------------------------------------------------------
// c--------------------------------------------------------------------------
// c--------------------------------------------------------------------------

int el_mgmt::
set_indices_and_wts( double x, double y, double z )
{
  // for this grid point
  //
  // first factor in the effect of the earth's curvature

  int mark;
  double r = sqrt( x * x + y * y );
  double zz = z - .5 * r * r * rcp_Rprime; // radar's z val

  double el = DEGREES( atan2( zz, r ) );
  last_elev = el;

  int current_ndx = (int)(( el - firstEl ) * rcp_el_inc);

  if( current_ndx < 0 || current_ndx >= size_table ) {
    return -1;          // no data for this level
  }
  el_ndx_a = *( ndx_a_lut + current_ndx );

  double z_a = *( tan_elev + el_ndx_a ) * r;
  double z_b = *( tan_elev + el_ndx_a +1 ) * r;
  double rcp_zdiff = 1./( z_b - z_a );
  wt_a = ( z_b - zz ) * rcp_zdiff;
  wt_b = fabs( zz - z_a ) * rcp_zdiff;

  if( wt_a < 0 || wt_b < 0 ) {
    mark = 0;
  }

  return el_ndx_a;
}

// c--------------------------------------------------------------------------
