// c--------------------------------------------------------------------------

void VCP_minder::
accept_vcp_line( char * line )
{
    nexrad_VCP_items * lnvi;
    nexrad_VCP_header * lnvh;

    if( *line == '#' )
        { return; }             // comment
        
        char str[16];
    if(!strncmp(line, "VCPAT", 5)) {
        // this is a new pattern 
        //
        vcp = nlist->add_node();
        lnvh = vcp->return_vcp_hdr();

        int ii = sscanf(line, "%s%d%d%d%d%d%x"
                        , str
                        , &lnvh->item_1
                        , &lnvh->pattern_type
                        , &lnvh->VCP_num
                        , &lnvh->num_sweeps
                        , &lnvh->item_5
                        , &lnvh->pulse_flag
            );
        /*
         * lower byte of lnvh->pulse_flag == 4 => long_pulse
         * lower byte of lnvh->pulse_flag == 2 => short_pulse
         *
         */
        return;
    }
    // assume the next line of sweep info for the current vcp;
    //
    lnvi = vcp->next_sweep();
        
    int ii = sscanf(line, "%x%d%d%d%x%x%d%d%x%x%d%d%x%d%d%d%d"
                , &lnvi->fixed_angle
                , &lnvi->wave_type
[63;1H[K:[63;1H[63;1H[K                , &lnvi->is_prf_num
                , &lnvi->is_pulse_count
                , &lnvi->az_rate
                , &lnvi->item_6
                , &lnvi->id_prf_num
                , &lnvi->id_pulse_count
                , &lnvi->item_9
                , &lnvi->item_10
                , &lnvi->item_11
                , &lnvi->item_12
                , &lnvi->item_13
                , &lnvi->item_14
                , &lnvi->item_15
                , &lnvi->item_16
                , &lnvi->item_17
        );
    lnvi->real_fixed_angle = lnvi->fixed_angle * NEX_FIXED_ANGLE;
    lnvi->real_az_rate = lnvi->az_rate * NEX_AZ_RATE;
    lnvi->is_prf_ndx = lnvi->is_prf_num -1;
    lnvi->id_prf_ndx = lnvi->id_prf_num -1;
    return;
}

// c---------------------------------------------------------------------------

void VCP_minder::
set_VCP( int the_vcp_num, int the_swp_num, int unamb_range )
{
    if( the_vcp_num != vcp_num ) {
        vcp = nlist->loop_reset();
        for(; vcp ; vcp = nlist->next_node() ) {
            if( vcp->return_vcp_hdr()->VCP_num == the_vcp_num )
                { break; }
        }
    }
    nvh = vcp->return_vcp_hdr();
    vcp_num = nvh->VCP_num;
    swp_num = the_swp_num -1;
    nvi = vcp->swp_info( swp_num );

    current_delta = -1;

    for( int ii = 0; ii < NEX_NUM_DELTAS; ii++ ) {
        for( int jj = 0; jj < NEX_NUM_UNAMB_RNGS; jj++) {
            if( unamb_range == nex_unamb_rngs[ii][jj] ) {
                current_delta = ii;
                break;
            }
        }
        if( current_delta >= 0 )
            { break; }
    }
}

// c---------------------------------------------------------------------------

void nexrad_site_stuff::
accept_site_line( char * line )
{
    if( *line == '#' )
        { return; }

[63;1H[K:[63;1H[63;1H[K    char sss[256], *sssp[32], *aa;
    strcpy( sss, line );
    int nt = dd_tokens( sss, sssp );
    NEXRAD_site * ns = next_site();

    ns->site_number = atoi( sssp[ site_num_ndx ] );

    strcpy( ns->site_name, sssp[ site_name_ndx ] );
    strcpy( ns->city, sssp[ city_ndx ] );
    strcpy( ns->state, sssp[ state_ndx ] );

    ns->latitude = atoi( sssp[ lat_deg ] );

    double frac = atof( sssp[ lat_min ] )/60. +
        atof( sssp[ lat_sec ] )/3600.;

    if( ns->latitude < 0 )
        { ns->latitude -= frac; }
    else
        { ns->latitude += frac; }

    ns->longitude = atoi( sssp[ long_deg ] );

    frac = atof( sssp[ long_min ] )/60. +
        atof( sssp[ long_sec ] )/3600.;

    if( ns->longitude < 0 )
        { ns->longitude -= frac; }
    else
        { ns->longitude += frac; }
    ns->altitude_mmsl = atof( sssp[ alt_ndx ] );

}

// c---------------------------------------------------------------------------

int nex_mapper::
map_ptrs( char * buf, int size, int * bytes_used )
{
    static int count = 0, count_trip = 2265;
    int mark = 0;
    char *at, *mark_drdh;
    char *aa;
    *bytes_used = 0;

    new_Ray = NO;

    for( ;; ) {
        count++;
        if( count >= count_trip ) {
            mark = 0;
        }
        if( size - *bytes_used < 8 ) {
            *bytes_used = size;
            return 0;
        }
        at = buf;

        if( !strncmp( at, "ARCH", 4 )) { // "ARCHIVE2"
            new_Ray = NO;
            int nn = strlen( "ARCHIVE2" );
            if( *(at +nn) == '.' ) {    // volume header record
[63;1H[K:[63;1H[63;1H[K                if( size == 24 ) {
                    *bytes_used = size;
                    return 0;
                }
                // no record boundaries
                buf += 24;
                *bytes_used += 24;
                continue;
            }
            else {
                mark = 0;
                char name[16];

                if( !strlen( nex_radar_name )) {
                    memset( name, 0, strlen( name ));
                    strncpy( name, at +nn, 4 );
                    mark = nss->site_info( name );
                }
                if( !nss->return_site() ) {
                    printf( "No site information for this tape for %s or %s\n"
                            , name, nex_radar_name );
                    exit( 1 );
                }

                if( size <= 31616 ) {
                    *bytes_used = size;
                    return 0;
                }
                // no record boundaries
                buf += 31616;
                *bytes_used += 31616;
                continue;
            }
        }
        memcpy( &ctm, at, sizeof( ctm ));
        at += sizeof( ctm );

        if( LittleEndian ) {
             nex_crack_nmh (at, (char *)&nmh, 0);
        }
        else {
            memcpy( &nmh, at, sizeof( nmh ));
        }
        at += sizeof( nmh );
        mark_drdh = at;

        if( nmh.message_type ==  RDA_STATUS_DATA ) {
            if( LittleEndian ) {
                nex_crack_rda (at, (char *)&rsi, 0);
            }
            else {
                memcpy( &rsi, at, sizeof( rsi ));
            }
            buf += NEX_PACKET_SIZE;
            *bytes_used += NEX_PACKET_SIZE;
            continue;
        }

        if( nmh.message_type != DIGITAL_RADAR_DATA ) {
            *bytes_used = size;
            return -1;
        }
[63;1H[K:[63;1H[63;1H[K        else if( ( size - *bytes_used ) < NEX_PACKET_SIZE ) {
            return 0;
        }
        new_Ray = YES;
        if( LittleEndian ) {
             nex_crack_drdh (at, (char *)&drdh, 0);
        }
        else {
            memcpy( &drdh, at, sizeof( drdh ));
        }
        *bytes_used += NEX_PACKET_SIZE;

        raw_data[NEX_DZ_ID] = NULL;
        raw_data[NEX_VE_ID] = NULL;

        if(drdh.ref_ptr <= 0 ||
           (drdh.ref_ptr > 0 && drdh.ref_ptr == drdh.vel_ptr)) {
            /* funky tapes from Atlanta; do nothing for now! */
            raw_data[NEX_DZ_ID] = NULL;
        }
        else {
            raw_data[NEX_DZ_ID] = mark_drdh + drdh.ref_ptr;
        }
        if( drdh.vel_ptr > 0 ) {
            raw_data[NEX_VE_ID] = mark_drdh + drdh.vel_ptr;
            raw_data[NEX_SW_ID] = mark_drdh + drdh.sw_ptr;
        }

        new_Vol = new_Sweep = NO;

        if( drdh.radial_status == BEGINNING_OF_VOL_SCAN) {
            new_Vol = new_Sweep = YES;
        }
        if( drdh.radial_status == START_OF_NEW_ELEVATION) {
            new_Sweep = YES;
        }    

        if( new_Sweep ) {
            if( new_Vol ) {
                ref_nyquist_vel_x100 = 0;
            }

            if( drdh.nyquist_vel_x100 > 0 ) {
                if( !ref_nyquist_vel_x100 ) {
                    ref_nyquist_vel_x100 = drdh.nyquist_vel_x100;
                }
                else if( drdh.nyquist_vel_x100 < ref_nyquist_vel_x100 ) {
                    ref_nyquist_vel_x100 = drdh.nyquist_vel_x100;
                }
            }

            vcpm->set_VCP( (int)drdh.vol_coverage_pattern
                           , (int)drdh.elev_num
                           , (int)(drdh.unamb_range_x10)/10 );

            if( raw_data[NEX_VE_ID] != NULL ) {
                if( drdh.velocity_resolution == ONE_METER_PER_SEC) {
                    d_scale[NEX_VE_ID] = rcp_scale[NEX_VE_ID] = 1.0;
                    d_bias[NEX_VE_ID] = -127.;
                }
                else {
                    d_scale[NEX_VE_ID] = 0.5;
[63;1H[K:[63;1H[63;1H[K                    rcp_scale[NEX_VE_ID] = 1.0/d_scale[NEX_VE_ID];
                    d_bias[NEX_VE_ID] = -63.5;
                }
            }
        }
        ddt->reset();
        ddt->set_year( 1970 );
        ddt->set_month( 0 );
        ddt->set_day( 0 );
        DTime dt = drdh.milliseconds_past_midnight * .001 +
            ( drdh.julian_date -1 ) * SECONDS_PER_DAY;
        ddt->set_additional_seconds( dt );
        dtime = ddt->stamp_the_time();
        ddt->unstamp_the_time();
//      aa = ddt->ascii_time();


        Num_fields = 0;
        if( raw_data[NEX_DZ_ID] != NULL ) {
            Num_fields++;
        }

        if( raw_data[NEX_VE_ID] != NULL ) {
            Num_fields += 2;
        }



        break;
    }
    return *bytes_used;
}

// c---------------------------------------------------------------------------

char * nex_mapper::
smear_it( int field_num )
{
  // routine to average gates to compensate for beam spreading with range
  int que[24], qndx, gg, good_gates, trip, navg;
  int * aptr, * tptr;
  int ia, nn, ival, mark;
  unsigned char * gates, * smeared;
  unsigned char * gt, * sm;
  double sum, davg;

  switch( field_num ) {

  case NEX_DZ_ID:
    tptr = ref_smear_gate_trip;
    aptr = ref_smear_avg;
    smeared = (unsigned char *)smeared_ref;
    gates = (unsigned char *)raw_data[ field_num ];
    break;

  case NEX_VE_ID:
    tptr = vel_smear_gate_trip;
    aptr = vel_smear_avg;
    smeared = (unsigned char *)smeared_vel;
    gates = (unsigned char *)raw_data[ field_num ];
    break;

[63;1H[K:[63;1H[63;1H[K  case NEX_SW_ID:
    break;
  }
  int max_gates = number_of_cells( field_num );
  trip = *tptr < max_gates ? *tptr : max_gates;
  tptr++;

  for( ia = gg = 0; gg < trip; ia++, gg++)
    { *(smeared +ia) = *(gates +gg); }

  for(; gg < max_gates; aptr++, tptr++ ) {
    navg = *aptr;
    trip = *tptr < max_gates ? *tptr : max_gates;
    gg = ia;
    gg -= navg/2;
    sum = qndx = good_gates = 0;
    gt = gates +ia;
    sm = smeared +ia;

    // load up the average que

    for( nn = navg; nn-- ; gg++, qndx++ ) {
      que[qndx] = ival = *( gates +gg );
      if( ival > 1 ) {
        sum += ival;
        good_gates++;
      }
    }
    mark = 0;

    for( qndx = 0; gg < trip ; gg++, ia++ ) {

      davg = good_gates ? sum/(double)good_gates : 0;
      *( smeared + ia ) = (unsigned char)davg;

      ival = que[qndx];
      if( ival > 1 ) {
        sum -= ival;
        good_gates--;
      }
      que[qndx] = ival = *( gates +gg );
      if( ival > 1 ) {
        sum += ival;
        good_gates++;
      }
      qndx = ++qndx % navg;
    }
  }
  mark = 0;

  for(; ia < max_gates; ia++ )  // finish out the array
    { *( smeared +ia ) = (unsigned char)davg; }

  return (char *)smeared;
}

// c---------------------------------------------------------------------------


// c---------------------------------------------------------------------------

